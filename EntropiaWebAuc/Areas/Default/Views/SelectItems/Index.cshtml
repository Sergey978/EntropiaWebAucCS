@model EntropiaWebAuc.Areas.Default.ViewModels.SelectItemsViewModel
@using EntropiaWebAuc.Domain.Entities



<div class="row">
    <div class="col-xs-6">


        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Custom Items</h3>
            </div>
            <div class="panel-body">
                @foreach (CustomItem item in Model.CustomItems)
                {
                    @item.Name<br>

                }
            </div>
        </div>
    </div>

    <div class="col-xs-6">


        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">You Selected Custom Items</h3>
            </div>
            <div class="panel-body">
                @foreach (CustomItem item in Model.SelectedCustomItems)
                {
                    @item.Name<br>

                }
            </div>
        </div>

    </div>
</div>

<hr>

<div class="row">
    <div class="col-xs-6">


        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Standart Items</h3>
            </div>
            <div class="panel-body">
                @foreach (StandartItem item in Model.StandartItems)
                {
                    @item.Name<br>

                }
            </div>
        </div>
    </div>

    <div class="col-xs-6">


        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">You Selected Standart Items</h3>
            </div>
            <div class="panel-body">
                @foreach (StandartItem item in Model.SelectedStandartItems)
                {
                    @item.Name<br>

                }
            </div>
        </div>

    </div>
</div>