<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@Page.Title</title>

    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/style2")


</head>
<body>
    @Html.Partial("_NavBar")
